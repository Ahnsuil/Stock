{
  "name": "stockflow",
  "version": "2.0.0",
  "description": "Stock Management Web Application with advanced expiry tracking, transfers, and discard management",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:prod": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "docker:frontend:build": "docker-compose build frontend",
    "docker:frontend:up": "docker-compose up -d frontend",
    "docker:frontend:stop": "docker-compose stop frontend",
    "docker:frontend:restart": "docker-compose up -d --build frontend",
    "docker:frontend:rebuild": "docker-compose build --no-cache frontend",
    "docker:frontend:logs": "docker-compose logs -f frontend",
    "docker:standalone:up": "docker-compose -f docker-compose.standalone-frontend.yml up -d --build",
    "docker:standalone:stop": "docker-compose -f docker-compose.standalone-frontend.yml stop"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.4",
    "vite": "^4.1.0"
  }
}
